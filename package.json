{
  "name": "risk-management",
  "version": "3.0.0",
  "description": "Hello world of sap",
  "dependencies": {
    "@sap-cloud-sdk/http-client": "2.15.0",
    "@sap-cloud-sdk/resilience": "3.12.0",
    "@sap/cds": "7.9.2",
    "@sap/cds-dk": "7.9.3",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.7.1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        },
        "schema_evolution": "auto"
      }
    }
  }
}
